############
# Secrets
# YOU MUST CHANGE THESE BEFORE GOING INTO PRODUCTION
############

# Generate using: openssl rand -base64 32
POSTGRES_PASSWORD=your_postgres_password_here
JWT_SECRET=your_jwt_secret_here

# Generate using Supabase JWT generator or https://supabase.com/docs/guides/self-hosting/docker
ANON_KEY=your_anon_key_here
SERVICE_ROLE_KEY=your_service_role_key_here

# Dashboard credentials
DASHBOARD_USERNAME=supabase
DASHBOARD_PASSWORD=your_dashboard_password_here

# Generate using: openssl rand -hex 64
SECRET_KEY_BASE=your_secret_key_base_here

# Generate using: openssl rand -base64 32
VAULT_ENC_KEY=your_vault_encryption_key_here


############
# Database - You can change these to any PostgreSQL database that has logical replication enabled.
############
POSTGRES_HOST=db
POSTGRES_DB=postgres
POSTGRES_PORT=5432
# default user is postgres


############
# Supavisor -- Database pooler
############
# Port Supavisor listens on for transaction pooling connections
POOLER_PROXY_PORT_TRANSACTION=6543
# Maximum number of PostgreSQL connections Supavisor opens per pool
POOLER_DEFAULT_POOL_SIZE=20
# Maximum number of client connections Supavisor accepts per pool
POOLER_MAX_CLIENT_CONN=100
# Unique tenant identifier
POOLER_TENANT_ID=your_tenant_id
# Pool size for internal metadata storage used by Supavisor
# This is separate from client connections and used only by Supavisor itself
POOLER_DB_POOL_SIZE=5


############
# API Proxy - Configuration for the Kong Reverse proxy.
############

KONG_HTTP_PORT=8000
KONG_HTTPS_PORT=8443


############
# API - Configuration for PostgREST.
############

PGRST_DB_SCHEMAS=public,storage,graphql_public


############
# Auth - Configuration for the GoTrue authentication server.
############

## General
SITE_URL=https://your-domain.com
ADDITIONAL_REDIRECT_URLS="https://your-domain.com,https://your-domain.com/**"
JWT_EXPIRY=3600
DISABLE_SIGNUP=false
API_EXTERNAL_URL=https://api.your-domain.com

## Mailer Config
MAILER_URLPATHS_CONFIRMATION="/auth/v1/verify"
MAILER_URLPATHS_INVITE="/auth/v1/verify"
MAILER_URLPATHS_RECOVERY="/auth/v1/verify"
MAILER_URLPATHS_EMAIL_CHANGE="/auth/v1/verify"

## Email auth
ENABLE_EMAIL_SIGNUP=true
ENABLE_EMAIL_AUTOCONFIRM=false
ENABLE_ANONYMOUS_USERS=true

# SMTP Configuration
SMTP_ADMIN_EMAIL=noreply@your-domain.com
SMTP_HOST=smtp.your-provider.com
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASS=your_smtp_password
SMTP_SENDER_NAME=YourAppName

## Phone auth
ENABLE_PHONE_SIGNUP=false
ENABLE_PHONE_AUTOCONFIRM=true


############
# Studio - Configuration for the Dashboard
############

STUDIO_DEFAULT_ORGANIZATION=Default Organization
STUDIO_DEFAULT_PROJECT=Default Project

STUDIO_PORT=3000
# replace if you intend to use Studio outside of localhost
SUPABASE_PUBLIC_URL=https://api.your-domain.com

# Enable webp support
IMGPROXY_ENABLE_WEBP_DETECTION=true

# Add your OpenAI API key to enable SQL Editor Assistant (optional)
OPENAI_API_KEY=your_openai_api_key_here


############
# Functions - Configuration for Functions
############
# NOTE: VERIFY_JWT applies to all functions. Per-function VERIFY_JWT is not supported yet.
FUNCTIONS_VERIFY_JWT=false


############
# Logs - Configuration for Analytics
# Please refer to https://supabase.com/docs/reference/self-hosting-analytics/introduction
############

# Change vector.toml sinks to reflect this change
# these cannot be the same value
# Generate using: openssl rand -hex 32
LOGFLARE_PUBLIC_ACCESS_TOKEN=your_public_access_token_here
LOGFLARE_PRIVATE_ACCESS_TOKEN=your_private_access_token_here

# Docker socket location - this value will differ depending on your OS
DOCKER_SOCKET_LOCATION=/var/run/docker.sock

# Google Cloud Project details (if using Google Cloud services)
GOOGLE_PROJECT_ID=your_project_id
GOOGLE_PROJECT_NUMBER=your_project_number


# Sequin Configuration
# Generate using: openssl rand -base64 32
SEQUIN_SECRET_KEY_BASE=your_sequin_secret_key_base
SEQUIN_POSTGRES_PASSWORD=your_sequin_postgres_password

# Typesense Configuration
# Generate using: openssl rand -base64 32
TYPESENSE_API_KEY=your_typesense_api_key
TYPESENSE_HOST=typesense
TYPESENSE_PORT=8108
TYPESENSE_PROTOCOL=http


############
# Store Supabase Instance (Auth-Only)
############

STORE_DASHBOARD_USERNAME=supabase
STORE_DASHBOARD_PASSWORD=your_store_dashboard_password

# Database Configuration
STORE_POSTGRES_HOST=store-db
STORE_POSTGRES_PASSWORD=your_store_postgres_password

# JWT Configuration (Generate new keys for security isolation)
STORE_JWT_SECRET=your_store_jwt_secret
STORE_ANON_KEY=your_store_anon_key
STORE_SERVICE_ROLE_KEY=your_store_service_role_key

# API Configuration
STORE_KONG_HTTP_PORT=8100
STORE_KONG_HTTPS_PORT=8543
STORE_API_EXTERNAL_URL=https://api-store.your-domain.com

# Auth Configuration
STORE_SITE_URL=http://localhost:3002
STORE_ADDITIONAL_REDIRECT_URLS="https://**"

# Studio Configuration
STORE_STUDIO_PORT=3000
STORE_SUPABASE_PUBLIC_URL=https://api-store.your-domain.com

# Analytics Configuration
# Generate using: openssl rand -hex 32
STORE_LOGFLARE_PUBLIC_ACCESS_TOKEN=your_store_public_token
STORE_LOGFLARE_PRIVATE_ACCESS_TOKEN=your_store_private_token

### CLICKHOUSE
CLICKHOUSE_HOST=clickhouse
CLICKHOUSE_PORT=8123
CLICKHOUSE_TCP_PORT=9000
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=your_clickhouse_password
CLICKHOUSE_DB=ecomweb

CLICKHOUSE_UI_PORT=5521


# Google OAuth (optional)
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret
